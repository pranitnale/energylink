{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/api/:path*"
      },
      {
        "source": "/:path*",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "routes": [
      { "handle": "filesystem" },
      { "src": "/[^.]+", "dest": "/", "status": 200 }
    ]
  }
  